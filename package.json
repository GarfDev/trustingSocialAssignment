    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.5",
    "react-hooks": "^1.0.1",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
