{
  "name": "garf-blog",
  "version": "1.0.0",
  "main": "app.tsx",
  "license": "MIT",
  "scripts": {
    "bundle": "webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --mode development --host 0.0.0.0 --hot -d --config webpack.config.dev.js",
    "test": "jest --passWithNoTests --collectCoverage",
    "test:watch": "jest --watch --verbose false",
    "build": "webpack --mode production --config webpack.config.prod.js && cp ./app/_redirects ./build/",
    "check-types": "tsc",
    "lint": "tsc --noEmit && eslint 'app/**/*.{ts,tsx}' --quiet --fix --max-warnings 0",
    "eslint": "eslint 'app/**/*.{ts,tsx}' --quiet --fix",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.{ts,tsx,jsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "jest"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv-webpack": "^1.7.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "immutable": "^4.0.0-rc.12",
