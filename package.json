    "react": "^0.0.0-experimental-7f28234f8",
    "react-dom": "^0.0.0-experimental-7f28234f8",
    "recoil": "^0.0.10",
    "seamless-immutable": "^7.1.4",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
